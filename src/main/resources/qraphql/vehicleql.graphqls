type Query {
    vehicles(count: Int): [Vehicle]
    vehicle(id: ID): Vehicle
}

type Mutation {
    createVehicle(
        type: String!,
        modelCode: String!,
        brandName: String,
        launchDate: String
    ): Vehicle
}

type Vehicle {
    id: ID!,
    type: String,
    modelCode: String,
    brandName: String,
    launchDate: String
}